
types
  set = (Forall x : TOSET . x |> Bool); 
in

let monoIf = 
  (forall src : SEMILATTICE .  
  (forall dest : SEMILATTICE . 
  (fun cond : src . 
  (fun res : dest . 
  
  let cons dest k v acc = cons unit cond (bot (Unit |> src)) in 
    res
  end
  ))))
and blang = 
  (forall src : SEMILATTICE .  
  (forall dest : SEMILATTICE . 
  (fun cond : src . 
  (fun res : dest . 
  
  let cons dest k v acc = cons unit cond (bot (Unit |> src)) in 
    res
  end
  ))))
in
  blang
end